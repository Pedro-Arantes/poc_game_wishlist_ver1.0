generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model games {
  id       Int     @id @default(autoincrement())
  name     String?
  platform String?
  genre    String?
  status   Boolean? @default(dbgenerated("false"))
  grade    Int?    @default(0)
}
